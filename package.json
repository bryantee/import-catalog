{
  "name": "import-catalog",
  "description": "Easily see an inventory of destructured imports from specific packages in your codebase",
  "version": "1.0.0",
  "main": "bin/index.js",
  "license": "MIT",
  "bin": {
    "import-catalog": "bin/index.js"
  },
  "scripts": {
    "start": "ts-node src/cli.ts",
    "build": "tsc",
    "test": "jest",
    "package": "pkg . --targets node14-macos-x64 --output import-catalog"
  },
  "dependencies": {
    "glob": "^8.0.1",
    "minimist": "^1.2.6"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.5.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.31",
    "jest": "^28.0.3",
    "pkg": "^5.6.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "pkg": {
    "scripts": "./build/**/*.js",
    "outputPath": "./dist"
  }
}
